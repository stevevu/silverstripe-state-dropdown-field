{
  "name": "dynamic/state-dropdown-field",
  "description": "A simple state dropdown field for SilverStripe forms",
  "authors": [
    {
      "name": "Dynamic",
      "email": "dev@dynamicagency.com"
    }
  ],
  "keywords": [
    "silverstripe",
    "dropdown",
    "state",
    "form",
    "field"
  ],
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/recipe-cms": "^4.0",
    "silverstripe/vendor-plugin": "^1.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "squizlabs/php_codesniffer": "*"
  },
  "config": {
    "process-timeout": 600
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\StateDropdownField\\": "src/",
      "Dynamic\\StateDropdownField\\Tests\\": "tests/"
    }
  },
  "extra": {
    "installer-name": "state-dropdown-field",
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
